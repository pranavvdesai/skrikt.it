<img src="<%=user.avatar%>" alt="<%=user.name%>" width="100" />

<%if(locals.user && locals.user.id == profile_user.id){%>
    <%= profile_user.name %>
    <%= profile_user.email %>
    
<form action="/users/updateprofile/<%= profile_user.id %>" enctype="multipart/form-data" method="POST">
<input type="text" name="profile_name" value="<%=profile_user.name%>" required>
<input type="email" name="profile_email" value="<%=profile_user.email%>" required>
<input type="file" name="avatar" placeholder="Profile picha">
<input type="submit" value="Update">

</form>



<%}else{%>
    

    <%= profile_user.name %>
    <%= profile_user.email %>
    

<%}%>